<h1>Welcome to app</h1>
<h2>Lists </h2>

<ol>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list %> | <%= link_to "Edit", edit_list_path(list) %> | <%= link_to "X", list, method: :delete, data: { confirm: 'Are you sure?' } %> </li>
  <% end %>
</ol>

<%= link_to "Add a list", new_list_path %>
